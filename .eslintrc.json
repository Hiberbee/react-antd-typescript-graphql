{
  "extends": ["plugin:@typescript-eslint/recommended", "plugin:react/recommended"],
  "ignorePatterns": ["build", ".storybook"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react-hooks"],
  "rules": {
    "@typescript-eslint/member-delimiter-style": "off"
  },
  "settings": {}
}
